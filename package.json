{
  "name": "lit-element-starter-ts",
  "version": "0.0.0",
  "description": "A simple web component",
  "main": "server/server.js",
  "private": true,
  "type": "module",
  "module": "server/server.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf my-element.{d.ts,d.ts.map,js,js.map} test/my-element.{d.ts,d.ts.map,js,js.map} test/my-element_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch",
    "start": "LOG_TIME=abs node --experimental-modules --es-module-specifier-resolution=node dist/server/server.js",
    "start:dev": "LOG_TIME=abs NODE_ENV=development node --inspect --experimental-modules --es-module-specifier-resolution=node dist/server/server.js",
    "webpack:watch": "webpack --watch --config webpack.dev.cjs",
    "webpack:build": "webpack --config webpack.prod.cjs",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat my-element.bundled.js | gzip -9 | wc -c ; rm my-element.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "Jeffrey Sun",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.1",
    "body-parser": "^1.19.0",
    "confetti-js": "0.0.18",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "json-size": "^1.0.0",
    "lit-element": "^2.3.1",
    "log": "^6.0.0",
    "log-node": "^7.0.0",
    "validator": "^13.1.1",
    "ws": "^7.3.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "html-webpack-plugin": "^4.3.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "source-map-loader": "^1.0.1",
    "terser-webpack-plugin": "^3.0.8",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "web-component-analyzer": "^1.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  }
}
